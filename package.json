{
  "name": "le-checkout-github",
  "version": "1.0.0",
  "description": "This is a Chrome browser extension that generates a git branch name based on the current GitHub issue key and the issue\n summary.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "check": "biome check ./",
    "format": "biome format --write ./",
    "release": "echo \"Error: no release specified\" && exit 1",
    "prepare": "lefthook install"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@biomejs/biome": "1.8.3",
    "lefthook": "^1.7.3"
  }
}
